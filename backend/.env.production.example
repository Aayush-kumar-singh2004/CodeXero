# Production Environment Variables
# Copy this file to .env.production and update with your actual values

# Server Configuration
PORT=3000
NODE_ENV=production
DOMAIN=localhost
# Database
DB_CONNECT_STRING=mongodb+srv://username:password@cluster.mongodb.net/database

# JWT
JWT_KEY=your-super-secure-jwt-secret-key-here

# Redis (if using)
REDIS_PASS=your-redis-password

# Judge0 API
JUDGE0_KEY=your-judge0-api-key

# Cloudinary
CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
CLOUDINARY_API_KEY=your-cloudinary-api-key
CLOUDINARY_API_SECRET=your-cloudinary-api-secret

# Gemini AI Keys
GEMINI_KEY=your-gemini-api-key-1
GEMINI_KEY2=your-gemini-api-key-2
GEMINI_KEY3=your-gemini-api-key-3
GEMINI_KEY4=your-gemini-api-key-4
GEMINI_KEY5=your-gemini-api-key-5
GEMINI_KEY6=your-gemini-api-key-6

# OAuth Configuration
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret

# Frontend Configuration
FRONTEND_URL=https://your-frontend-domain.com
CORS_ORIGIN=https://your-frontend-domain.com